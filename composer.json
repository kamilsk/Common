{
  "name": "kamilsk/common",
  "type": "library",
  "description": "Common codebase.",
  "homepage": "https://github.com/kamilsk/Common",
  "license": "MIT",
  "authors": [
    { "name": "Kamil Samigullin", "email": "kamil@samigullin.info", "role": "lead" }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "composer/composer": "~1.0",
    "doctrine/migrations": "~1.0",
    "jolicode/jolinotif": "~1.0",
    "monolog/monolog": "~1.0",
    "symfony/config": "~3.0",
    "symfony/yaml": "~3.0"
  },
  "suggest": {
    "doctrine/migrations": "for use OctoLab\\Common\\Doctrine\\Migration (~1.0 is recommended)",
    "jolicode/jolinotif": "optional for show desktop notification (~1.0 is recommended)",
    "monolog/monolog": "for use OctoLab\\Common\\Monolog (~1.0 is recommended)",
    "symfony/config": "for use OctoLab\\Common\\Config (~3.0 is recommended)",
    "symfony/filesystem": "for use OctoLab\\Common\\Composer\\Script\\AdminLte (~3.0 is recommended)",
    "symfony/yaml": "for use OctoLab\\Common\\Config (~3.0 is recommended)"
  },
  "autoload": {
    "psr-4": { "OctoLab\\Common\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "OctoLab\\Common\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev",
      "dev-3.x": "3.x-dev"
    }
  },
  "support": {
    "irc": "irc://irc.gitter.im/kamilsk/small-tools",
    "issues": "https://github.com/kamilsk/Common/issues"
  }
}
