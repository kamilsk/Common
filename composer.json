{
  "name": "kamilsk/common",
  "type": "library",
  "description": "Common codebase.",
  "homepage": "https://github.com/kamilsk/Common",
  "license": "MIT",
  "authors": [
    { "name": "Kamil Samigullin", "email": "kamil@samigullin.info", "role": "lead" }
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "doctrine/migrations": "~1.0",
    "monolog/monolog": "~1.0",
    "secondparty/dipper": "*",
    "symfony/config": "^2.0",
    "symfony/yaml": "^2.0"
  },
  "suggest": {
    "doctrine/migrations": "for use OctoLab\\Common\\Doctrine\\Migration (~1.0 is recommended)",
    "incenteev/composer-parameter-handler": "to handle parameters.yml (~2.0 is recommended)",
    "monolog/monolog": "for use OctoLab\\Common\\Monolog (~1.0 is recommended)",
    "secondparty/dipper": "as alternative of Symfony Yaml parser (stable version is recommended)",
    "symfony/config": "for use OctoLab\\Common\\Config (^2.0 is recommended)",
    "symfony/yaml": "for use OctoLab\\Common\\Config (^2.0 is recommended)"
  },
  "autoload": {
    "psr-4": { "OctoLab\\Common\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Test\\OctoLab\\Common\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
